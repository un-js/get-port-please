{
  "name": "get-port-please",
  "version": "2.6.1",
  "description": "Get an available TCP port to listen",
  "repository": "unjs/get-port-please",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext ts .",
    "prepack": "unbuild",
    "release": "pnpm test && pnpm build && standard-version && pnpm publish && git push --follow-tags",
    "test": "pnpm lint && vitest run"
  },
  "dependencies": {
    "fs-memo": "^1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "latest",
    "c8": "latest",
    "eslint": "latest",
    "jiti": "^1.16.0",
    "standard-version": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "^0.24.0"
  },
  "packageManager": "pnpm@7.13.3"
}
